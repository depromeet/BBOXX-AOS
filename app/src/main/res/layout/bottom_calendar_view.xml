<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_back"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true"
        android:background="@color/color_9D9D9D">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="357dp"
            android:background="@drawable/bg_top_30"
            android:backgroundTint="@color/white"
            android:clickable="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <LinearLayout
                android:id="@+id/lay_cal_title"
                android:layout_width="109dp"
                android:layout_height="24dp"
                android:layout_marginTop="40dp"
                android:orientation="horizontal"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.appcompat.widget.AppCompatImageButton
                    android:id="@+id/btn_cal_left"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ic_left_large"
                    android:layout_weight="1" />

                <TextView
                    android:id="@+id/txt_year"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textSize="18sp"
                    android:text="2021"
                    android:textColor="@color/mypage_bg"
                    android:layout_gravity="center"
                    android:layout_weight="1" />

                <androidx.appcompat.widget.AppCompatImageButton
                    android:id="@+id/btn_cal_right"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ic_right_large"
                    android:layout_weight="1" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="31dp"
                android:layout_marginBottom="50dp"
                android:layout_marginHorizontal="44dp"
                android:orientation="vertical"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lay_cal_title">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txt_one"
                        android:layout_height="60dp"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:text="1월"
                        android:textSize="16sp"
                        android:gravity="center"
                        android:layout_marginEnd="16dp"
                        android:textColor="@color/white"
                        android:backgroundTint="@color/card_view_1"
                        android:background="@drawable/bg_select_feeling_circle" />

                    <TextView
                        android:id="@+id/txt_two"
                        android:layout_height="60dp"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:text="2월"
                        android:textSize="16sp"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:layout_marginEnd="16dp"
                        android:backgroundTint="@color/card_view_2"
                        android:background="@drawable/bg_select_feeling_circle" />

                    <TextView
                        android:id="@+id/txt_three"
                        android:layout_height="60dp"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:text="3월"
                        android:textSize="16sp"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:layout_marginEnd="16dp"
                        android:backgroundTint="@color/card_view_3"
                        android:background="@drawable/bg_select_feeling_circle" />

                    <TextView
                        android:id="@+id/txt_four"
                        android:layout_height="60dp"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:text="4월"
                        android:textSize="16sp"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:backgroundTint="@color/card_view_4"
                        android:background="@drawable/bg_select_feeling_circle" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:layout_marginTop="16dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txt_five"
                        android:layout_height="60dp"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:text="5월"
                        android:textSize="16sp"
                        android:gravity="center"
                        android:layout_marginEnd="16dp"
                        android:textColor="@color/white"
                        android:backgroundTint="@color/card_view_5"
                        android:background="@drawable/bg_select_feeling_circle" />

                    <TextView
                        android:id="@+id/txt_six"
                        android:layout_height="60dp"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:text="6월"
                        android:textSize="16sp"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:layout_marginEnd="16dp"
                        android:backgroundTint="@color/card_view_6"
                        android:background="@drawable/bg_select_feeling_circle" />

                    <TextView
                        android:id="@+id/txt_seven"
                        android:layout_height="60dp"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:text="7월"
                        android:textSize="16sp"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:layout_marginEnd="16dp"
                        android:backgroundTint="@color/card_view_7"
                        android:background="@drawable/bg_select_feeling_circle" />

                    <TextView
                        android:id="@+id/txt_eight"
                        android:layout_height="60dp"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:text="8월"
                        android:textSize="16sp"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:backgroundTint="@color/card_view_8"
                        android:background="@drawable/bg_select_feeling_circle" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:layout_marginTop="16dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txt_nine"
                        android:layout_height="60dp"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:text="9월"
                        android:textSize="16sp"
                        android:gravity="center"
                        android:layout_marginEnd="16dp"
                        android:textColor="@color/white"
                        android:backgroundTint="@color/card_view_9"
                        android:background="@drawable/bg_select_feeling_circle" />

                    <TextView
                        android:id="@+id/txt_ten"
                        android:layout_height="60dp"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:text="10월"
                        android:textSize="16sp"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:layout_marginEnd="16dp"
                        android:backgroundTint="@color/card_view_10"
                        android:background="@drawable/bg_select_feeling_circle" />

                    <TextView
                        android:id="@+id/txt_eleven"
                        android:layout_height="60dp"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:text="11월"
                        android:textSize="16sp"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:layout_marginEnd="16dp"
                        android:backgroundTint="@color/card_view_11"
                        android:background="@drawable/bg_select_feeling_circle" />

                    <TextView
                        android:id="@+id/txt_twelve"
                        android:layout_height="60dp"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:text="12월"
                        android:textSize="16sp"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:backgroundTint="@color/card_view_12"
                        android:background="@drawable/bg_select_feeling_circle" />
                </LinearLayout>

            </LinearLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
